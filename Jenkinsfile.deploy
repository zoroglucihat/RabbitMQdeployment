pipeline {



  agent any

  stages {

    stage('Checkout Source') {
      steps {
        git 'https://github.com/zorolos/RabbitMQdeployment.git'
      }
    }





    stage('Deploy cluster-operator') {
      steps {
        script {
          kubernetesDeploy(configs: "cluster-operator.yaml", kubeconfigId: "mykubeconfig")
        }
      }
    }

  }

}
    stage('Deploy rabbitmq-cluster') {
      steps {
        script {
          kubernetesDeploy(configs: "rabbitmq-cluster.yaml", kubeconfigId: "mykubeconfig")
        }
      }
    }

  }

}
